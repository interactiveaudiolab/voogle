{
  "name": "vocalsearch",
  "version": "1.0.0",
  "description": "VocalSearch is an audio search engine that uses vocal imitations of the desired sound as the search query.",
  "main": "scripts/js/index.js",
  "scripts": {
    "test": "python -m unittest discover",
    "build": "webpack --mode production",
    "start": "webpack-dev-server --open --mode development",
    "production": "npm run build && npm run serve",
    "development": "npm run start && npm run serve-dev",
    "serve": "python app.py",
    "serve-dev": "python app.py -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/interactiveaudiolab/vocalsearch.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/interactiveaudiolab/vocalsearch/issues"
  },
  "homepage": "https://github.com/interactiveaudiolab/vocalsearch#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "bootstrap": "^4.1.3",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "inline-worker": "^1.1.0",
    "popper.js": "^1.14.4",
    "react-circular-progressbar": "^1.0.0",
    "wav-encoder": "^1.3.0",
    "wavesurfer.js": "^2.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
